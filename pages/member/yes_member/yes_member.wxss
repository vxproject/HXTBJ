/* 公共样式 */

.mar_center {
  margin: 0 auto;
}

.huizhang {
  position: absolute;
  top: 153rpx;
  right: 78rpx;
  width: 254rpx;
}

.header {
  position: relative;
}

.notice_box {
  background-color: #fff;
  border-radius: 14rpx;
  margin-top: 26rpx;
}

.name {
  color: #8f622f;
  width: 350rpx;
}

.header_banner {
  width: 100%;
  height: 300rpx;
}

.header_img {
  width: 100%;
  display: block;
}

.header_tubiao {
  color: #f8f0df;
  position: absolute;
  top: 104rpx;
  right: 32rpx;
  padding: 8rpx 20rpx;
  border-top-right-radius: 25rpx;
  border-bottom-left-radius: 25rpx;
  background: linear-gradient(to right, #b59567 0%, #ddb467 100%);
  display: flex;
  align-items: center;
}

.header_tubiao_home_logo {
  margin-right: 15rpx;
  width: 47rpx;
  height: 42rpx;
  vertical-align: middle;
}

.header_lingpai_logo {
  width: 28rpx;
  height: 30rpx;
  margin-left: 10rpx;
}

.header_xiaoxiao {
  width: 14rpx;
  height: 16rpx;
  margin-left: 10rpx;
}

.vip_img {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  margin-top: 14rpx;
}

.vip_img_cont {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;
  width: 132rpx;
  height: 38rpx;
  background: linear-gradient(to right, #ddb467 0%, #c4a57a 100%);
  border-radius: 26rpx;
  color: #fff;
}

.vip_img_cont_img {
  width: 29rpx;
  height: 26rpx;
  margin-right: 15rpx;
}

/* 用户信息框 */

.user_vip_logo {
  position: absolute;
  top: 17rpx;
  left: 390rpx;
  width: 60rpx;
  height: 49.333rpx;
}

.invite_code_box {
  margin: 0 auto;
  border: solid 4rpx #dcb66d;
  border-radius: 40rpx;
  height: 70rpx;
  line-height: 70rpx;
  width: 400rpx;
}

.header_user_photo {
  position: absolute;
  top: -87rpx;
  left: 50%;
  transform: translateX(-50%);
  display: block;
  width: 140rpx;
  height: 140rpx;
  background-color: green;
  border-radius: 50%;
}

.header_user_name {
  padding-top: 70rpx;
  font-weight: 600;
}

.header_user_photo, .header_user_name, .header_user_tuijian {
  color: #8f622f;
  margin-bottom: 20rpx;
  text-align: center;
  font-weight: bold;
}

.copy_btn {
  float: right;
  border-top-right-radius: 40rpx;
  border-bottom-right-radius: 40rpx;
  padding: 0rpx 20rpx;
  background-color: #dcb66d;
}

/* tabbar数据展示 */

/* tabbar数据展示 */

.tab_show {
  /* padding: 40rpx 0; */
  display: flex;
  display: -webkit-flex;
  justify-content: space-around;
  height: 152rpx;
  background-color: #fff;
  border-radius: 14rpx;
  align-items: center;
}

.color_text {
  color: #7d7d7d;
}

.color_num {
  color: #a77e3c;
  font-size: 36rpx;
}

.tab_show_item {
  text-align: center;
  width: 130rpx;
}

.tab_show_item>view:last-child {
  padding-top: 10rpx;
}

.container_group {
  padding: 30rpx;
}

/* 图片导航 */

.img_tab {
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  padding: 30rpx 0;
  align-items: baseline;
  flex-flow: row nowrap;
}

.img_tab_jiangli {
  width: 340rpx;
  height: 146rpx;
}

.img_tab_fanli {
  width: 340rpx;
  height: 142rpx;
}

/* 收入数据展示 */

.shouru_data {
  background-color: #fff;
  border-radius: 14rpx;
}

.shouru_data_header {
  padding: 30rpx;
  display: flex;
  align-items: center;
  flex-flow: row nowrap;
  justify-content: space-between;
  font-size: 30rpx;
  font-weight: bold;
  color: #333;
  border-bottom: 1rpx solid #f5f5f5;
}

.shouru_data_header>image {
  width: 40rpx;
  height: 40rpx;
}

.model_yuji_modle {
  display: flex;
  flex-flow: row wrap;
  padding:12rpx 30rpx;
}

.model_yuji {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  width: 50%;
  justify-content: center;
  padding: 30rpx 0;
}

.model_yuji_img {
  height: 58rpx;
  width: 57rpx;
  margin-right: 28rpx;
}

.shouru_data_item {
  margin-top: 5rpx;
  font-size: 24rpx;
  color: #7d7d7d;
}

/* 余额和体现 */

.tixian {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  padding: 0 30rpx;
  height: 124rpx;
  background-color: #fff;
  border-radius: 14rpx;
  align-items: center;
  margin-top: 26rpx;
  font-weight: bold;

}


.tixian_xianjin {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  font-size: 30rpx;
}

/* 商学院 */

.xueyuan_box {
  background-color: #fff;
  border-radius: 14rpx;
  margin-top: 26rpx;
}
.sx_ziti{
  font-size:28rpx;
  color: #666;
}
.xueyuan_header {
  font-size: 26rpx;
  color: #333;
}

.sx_xueyuan_header {
  font-size: 30rpx;
  font-weight: bold;
  color: #333;
  padding: 30rpx;
  border-bottom: 1rpx solid #f5f5f5;
}

.xueyuan_pro {
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  padding: 50rpx 30rpx;
}

.xueyuan_pro_item {
  text-align: center;
  width: 130rpx;
}

.xueyuan_pro_item_img {
  display: block;
  margin: 0 auto;
  width: 75rpx;
  height: 75rpx;
}

/* 公告 */

.notice_item {
  padding: 30rpx;
  border-bottom: 1rpx solid #f5f5f5;
}

.notice_header {
  padding: 30rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #333;
  border-bottom: 1rpx solid #f5f5f5;
}

.notice_item_title {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 20rpx;
  font-size: 26rpx;
  color: #333;
}

.notice_item_new {
  display: flex;
  align-items: center;
  padding: 0 9rpx;
  margin-right: 15rpx;
  background-color: #c8ac73;
  color: #fff;
  border-radius: 3rpx;
  font-size: 22rpx;
}

.shij_sty {
  font-size: 20rpx;
  color: #666;
}

.yudu_str {
  font-size: 22rpx;
  color: #666;
}

.notice_item_data {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center;
}

.datanotiic {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
}
