  border-radius: 20rpx;
  margin-top: 48rpx;
  padding-bottom: 20rpx;
  position: relative;
}

.kjbImage {
  width: 438rpx;
  height: 32rpx;
  margin-left: 106rpx;
  margin-top: 40rpx;
}

.kjbbg {
  width: 240rpx;
}

.moneybg {
  width: 250rpx;
  /* line-height: 130rpx; */
  display: flex;
  align-items: center;
}

.moneybg image {
  width: 50rpx;
  height: 53rpx;
  padding-right: 20rpx;
  padding-left: 20rpx;
}

.more {
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}

.more image {
