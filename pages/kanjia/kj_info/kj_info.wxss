/* pages/kanjia/kj_info/kj_info.wxss */
@import "/template/tanchuang/tanchuang.wxss";

page {
  background-color: #fe7a19;
}

.kj_info {
  height: 100%;
  width: 750rpx;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}

/* 用户头像 */

.userInfo {
  width: 100%;
  height: 130rpx;
  position: relative;
  display: flex;
}

.touxiang {
  width: 80rpx;
  height: 80rpx;
  border: 4rpx solid red;
  border-radius: 50%;
  margin-top: 17rpx;
  margin-left: 40rpx;
}

.miaosu {
  width: 470rpx;
  margin-top: 24rpx;
  margin-left: 20rpx;
}

.title {
  /* font-weight: bold; */
  line-height: 40rpx;
}

.zhaiyao {
  line-height: 40rpx;
}

.shangpin {
  width: 620rpx;
  margin: 0 auto;
  background-color: yellow;
  /* padding: 10rpx 10rpx; */
}

.top {
  border-radius: 15rpx;
  background-color: white;
  margin: 20rpx auto;
  width: 694rpx;
  padding-top: 26rpx;
  padding-bottom: 26rpx;
  /* height: 400rpx;  */
}

.goods {
  display: flex;
  padding: 10rpx 0rpx 10rpx 10rpx;
  width: 650rpx;
  margin: 0 auto;
  background-color: #f5f0f4;
  position: relative;
  border-radius: 20rpx;
}

.shou {
  position: absolute;
  left: 26rpx;
  top: 100rpx;
}

.price {
  position: absolute;
  left: 26rpx;
  bottom: 10rpx;
}

.xiaoliang {
  position: absolute;
  right: 26rpx;
  bottom: 10rpx;
}

.goodImage {
  width: 200rpx;
  height: 200rpx;
  border-radius: 15rpx;
}

.goodsInfo {
  width: 399rpx;
  height: 200rpx;
  padding-left: 26rpx;
  /* padding-right: 30rpx; */
  position: relative;
}

.shouhou_btn {
  position: absolute;
  right: 0rpx;
  bottom: 10rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  background-color: #ff5000;
}

.time {
  background-color: #ff5000;
  border-radius: 4rpx;
  color: white;
  padding: 5rpx 5rpx;
}

.over {
  display: flex;
}

.overtxt {
  margin-top: 10rpx;
  margin-left: 10rpx;
}

.mh {
  padding: 0rpx 5rpx;
}

.shstate {
  border-bottom-left-radius: 40rpx;
  border-top-left-radius: 40rpx;
}

/* 砍价 */

.kanjia {
  text-align: center;
  margin-top: 24rpx;
}

.kjbheight {
  max-height: 260rpx;
  overflow: hidden;
}

.progress {
  border-radius: 40rpx;
  width: 586rpx;
  margin-left: 54rpx;
  margin-top: 32rpx;
}

.sharezy {
  margin-top: 36rpx;
}

.sure {
  width: 574rpx;
  margin: 0 auto;
  margin-top: 22rpx;
  height: 90rpx;
  color: #fff600;
  /* background-color: #FF54AA; */
  border-radius: 40rpx;
  line-height: 90rpx;
}

.stateImage {
  width: 572rpx;
  /* height: 92rpx; */
  max-height: 92rpx;
  margin: 0 auto;
  margin-top: 22rpx;
}

/* 砍价帮 */

.kjbang {
  width: 650rpx;
  margin: 0 auto;
  /* overflow: hidden; */
