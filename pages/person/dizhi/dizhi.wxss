.search_container{
    height: 80rpx;
    padding-top: 10rpx;
}
.search_box{
    position: relative;
    width: 710rpx;
    height: 70rpx;
    margin: 0rpx auto;
    background-color: #eeeeee;
    display: flex;
    align-items: center;
    border-radius: 10rpx;
}
.search_box>image{
    display: inline-block;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    padding: 22rpx;
    width: 40rpx;
    height: 40rpx;
}
.search_box>input{
    position: absolute;
    top: 50%;
    left: 68rpx;
    transform: translateY(-50%);
    width: 596rpx;
    height: 70rpx;
    display: inline-block;
}
/* 无地址的内容 */
.no_address{
    position: absolute;
    top: 240rpx;
    left: 0;
    right: 0;
}
.no_address image{
    display: block;
    margin: 0 auto;
    width: 127rpx;
    height: 150rpx;
}
.no_address view{
    text-align: center;
    padding-top: 70rpx;
    color: #535353;
}
/* 有地址的状态 */
.yes_address{
    width: 690rpx;
    margin: 0 auto;
    padding: 46rpx 0 130rpx 0;
}
.yes_address_item{
    position: relative;
    padding-bottom: 50rpx;
   
    /* background-color: wheat; */
}
.yes_address_item image{
  width: 32rpx;
  height: 34rpx;
  position: absolute;
  right:-20rpx;
  top: 10rpx;
  padding: 20rpx;
}
.name_tel{
  display: flex;
  align-items: center;

}
.name_tel view{
border:solid 1rpx #fea593;
border-radius:10rpx;
width: 60rpx;
font-size: 22rpx;
color:#fd4722;
text-align: center;
height: 32rpx;
line-height: 32rpx;
margin-right: 16rpx
}
.name_tel text{
    /* padding-left: 16rpx; */

}
.name_tel text:last-child{
  padding-left: 26rpx;
}
.name_class{
  font-size: 28rpx;
  color:#333;
  font-weight: bold;
}
.tel_class{
  font-size:26rpx;
  color:#999999;
}
.xiangxi_address{
    width: 580rpx;
}
.bianji{
    position: absolute;
    right: 0;
    top: 50%;
    transform: translateY(-55%);
    /* border-left: solid 2rpx currentColor; */
