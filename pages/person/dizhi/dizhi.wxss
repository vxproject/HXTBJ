/* 底部按钮 */
.btn_container{
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 130rpx;
    background-color: #ffffff;
}
.btn_container button{
    position: absolute;
    top: 20rpx;
    left: 30rpx;
    width: 690rpx;
    height: 90rpx;
    line-height: 90rpx;
    margin: 0 auto;
    background-color: #ff5000;
    color: #ffffff;
    font-size: 30rpx;
    border-radius: 80rpx;
}
