@import "../../person/order/order.wxss";

/*  退款导航页面*/

/* 商品列表 */

.tuikuan_dh_goods {
  background-color: #fff;
  margin-top: 20rpx;
}

.goods_info_title {
  padding-top: 10rpx;
  font-size: 24rpx;
  color: #333;
}

.arr_cancel_btn {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 26rpx 0 20rpx 0;
}

.yichu_dot {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.goods_item {
  width: 550rpx;
  height: 200rpx;
  margin: 0 auto;
  display: flex;
  display: -webkit-flex;
  align-items: center;
}

/* 退款点击通道 */

.tuikuan_road_box {
  height: 120rpx;
  padding: 0 20rpx;
  display: flex;
  align-items: center;
  background-color: #fff;
  position: relative;
}

.tuikuan_road_logo {
  width: 45rpx;
  height: 45rpx;
}

.tuikuan_road_info {
  width: 560rpx;
  height: 100rpx;
  display: flex;
  /* flex-wrap: wrap; */
  margin-left: 34rpx;
  flex-direction: column;
  justify-content: center;
  /* align-content: space-between; */
}

.tuikuan_road_go {
  width: 35rpx;
  height: 35rpx;
  position: absolute;
  right: 20rpx;
}

.title {
  font-size: 26rpx;
  color: #333;
  padding-bottom: 10rpx;
}

.zhaiyao {
  padding-top: 10rpx;
  font-size: 22rpx;
  color: #999;
}

.btn_container {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 130rpx;
  background-color: #fff;
}

.btn_container button {
  position: absolute;
  top: 20rpx;
  left: 30rpx;
  width: 690rpx;
  height: 90rpx;
  line-height: 90rpx;
  margin: 0 auto;
  background-color: #ff5000;
  color: #fff;
  font-size: 30rpx;
  border-radius: 80rpx;
}

/* 售后说明 */

.shuoming_box {
  height: 105rpx;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
  border-top: solid 3rpx #ccc;
}

.shuoming_btn {
  width: 160rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 10rpx;
  margin-right: 35rpx;
  border: solid 3rpx currentColor;
}

