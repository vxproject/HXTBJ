  z-index: 2000;
  pointer-events: none;
}

.mode_wind {
  background: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  z-index: 2500;
 
}
.none{
   pointer-events: none;
}
.auto{
  pointer-events: auto;
}
