/* pages/person/shoucang_list/shoucang_list.wxss */
@import "/template/no_data/no_data.wxss";

page {
  background-color: #f5f5f5;
}

.gunali {
  position: fixed;
  top: 0rpx;
  right: 0;
  display: flex;
  padding: 10rpx 30rpx;
  /* background-color: #ff5767; */
  color:#ff5000;
  /* border-bottom-left-radius: 40rpx;
  border-top-left-radius: 40rpx; */
  align-items: center;
  line-height: 55rpx;
  font-size: 30rpx;
  z-index: 100;
}
.tmbg{
  background-color: black;
  opacity: 0.5;
}

.pos_bottom{
  position: fixed;
  left: 0;
  display: flex;
  width: 100%;
  background-color: #fff;
  z-index: 500;
  border-top:solid 2rpx #f5f5f5;
}

.translate {
  transition: all 0.5s linear;
}

.bot_cont {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 30rpx;
  width: 100%;
}
.ceshi{
  width:710rpx;
  margin: 0 auto;
  overflow-x:hidden;
}

.bot_left {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
}



.deleall {
  color: #ff6413;
  border-radius: 20rpx;

  /* padding: 17rpx 30rpx; */
  width: 120rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  border: solid 1rpx #ff6413;

}


/* 状态栏 */

.states{
  width: 100%;
  /* height: 80rpx; */
  background-color: #fff;
  display: flex;
  font-size: 30rpx;
  color:#333;
  position: fixed;
  top:0rpx;
  left:0rpx;
  z-index:99;
}
.states image{
  width: 10rpx;
