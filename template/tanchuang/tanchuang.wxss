.kanjia_content {
  position: fixed;
  height: 100%;
  width: 100%;
  overflow: auto;
  top: 0;
  left: 0;
}

.showmodel_wind {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  opacity: 1;
  background-color: rgba(0, 0, 0, 0.4);
  pointer-events: auto;
  z-index: 999;
  transition: all 0.3s;
  display: flex;
  justify-content: center;
  font-size: 30rpx;
}

.transform {
  transform: scale(1.1);
  opacity: 0;
  pointer-events: none;
}

.content {
  width: 486rpx;
  display: flex;
  flex-flow: column nowrap;
  justify-content: center;
}

.content_small {
  display: flex;
  flex-direction: row-reverse;
  margin-bottom: 50rpx;
  width: 100%;
}

.content_small_hezi {
  border-radius: 50%;
  width: 40rpx;
  height: 40rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
}

.content_big_img {
  display: block;
  height: 747.7rpx;
  /* background: #f5f5f5; */
  width: 100%;
  border-radius: 15rpx;
  flex-shrink: 0;
}

.content_small_img {
  flex-shrink: 0;
  width: 48rpx;
  height: 48rpx;
}

