/* 确认地址弹窗样式 */

.queren {
  position: fixed;
  width: 80%;
  left: 10%;
  top: 330rpx;
  height: auto;
  border-radius: 10rpx;
  background-color: #fff;
}

.queren_title {
  padding: 30rpx;
  display: flex;
  justify-content: center;
}

.querencontern {
  display: flex;
  justify-content: center;
}

.queren_cont {
  height: 80rpx;
  width: 75%;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all;
  background-color: #fff;
}

.queren_btn {
  margin-top: 45rpx;
  display: flex;
  flex-flow: row nowrap;
}

.cancel_btn {
  width: 50%;
  padding: 30rpx 0;
  display: flex;
  justify-content: center;
  border-right: 1rpx solid #d4d2d2;
  border-top: 1rpx solid #d4d2d2;
}

.confirm_btn {
  width: 50%;
  padding: 30rpx 0;
  display: flex;
  background-color: #fff;
  justify-content: center;
  border-top: 1rpx solid #d4d2d2;
}

